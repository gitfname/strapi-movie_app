{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "category",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "maxLength": 120,
      "minLength": 3
    },
    "movies": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::movie.movie",
      "inversedBy": "categories"
    },
    "popularity": {
      "type": "integer",
      "default": 1,
      "max": 10,
      "min": 1
    },
    "tv_shows": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tv-show.tv-show",
      "mappedBy": "categories"
    }
  }
}
