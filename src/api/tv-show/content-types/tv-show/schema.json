{
  "kind": "collectionType",
  "collectionName": "tv_shows",
  "info": {
    "singularName": "tv-show",
    "pluralName": "tv-shows",
    "displayName": "tv_show",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "text",
      "required": true,
      "maxLength": 400,
      "minLength": 3
    },
    "description": {
      "type": "text",
      "maxLength": 900,
      "minLength": 3
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "imdb": {
      "type": "integer",
      "required": true,
      "default": 1,
      "max": 10,
      "min": 1
    },
    "trailer": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "videos"
      ]
    },
    "actors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::actor.actor",
      "inversedBy": "tv_shows"
    },
    "actresses": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::actress.actress",
      "inversedBy": "tv_shows"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "tv_shows"
    },
    "tv_show_likes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tv-show-like.tv-show-like",
      "mappedBy": "tv_show"
    },
    "tv_show_comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tv-show-comment.tv-show-comment",
      "mappedBy": "tv_show"
    }
  }
}
