{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "movie_comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::movie-comment.movie-comment",
      "mappedBy": "owner"
    },
    "movie_likes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::movie-like.movie-like",
      "mappedBy": "owner"
    },
    "movie_comment_likes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::movie-comment-like.movie-comment-like",
      "mappedBy": "owner"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "favourite_movies": {
      "type": "json",
      "required": false
    },
    "favourite_categories": {
      "type": "json"
    },
    "actress_likes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::actress-like.actress-like",
      "mappedBy": "owner"
    },
    "actor_likes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::actor-like.actor-like",
      "mappedBy": "owner"
    },
    "movie_comment_replies": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::movie-comment-reply.movie-comment-reply",
      "mappedBy": "owner"
    },
    "movie_comment_reply_likes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::movie-comment-reply-like.movie-comment-reply-like",
      "mappedBy": "owner"
    },
    "tv_show_likes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tv-show-like.tv-show-like",
      "mappedBy": "owner"
    },
    "tv_show_comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tv-show-comment.tv-show-comment",
      "mappedBy": "owner"
    },
    "tv_show_comment_likes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tv-show-comment-like.tv-show-comment-like",
      "mappedBy": "owner"
    },
    "tv_show_comment_replies": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tv-show-comment-reply.tv-show-comment-reply",
      "mappedBy": "owner"
    },
    "tv_show_comment_reply_likes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tv-show-comment-reply-like.tv-show-comment-reply-like",
      "mappedBy": "owner"
    }
  }
}
